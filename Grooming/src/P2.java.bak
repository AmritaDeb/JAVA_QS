
/*
	    1
	   1*
	  1*2
	 1*2*
	1*2*3
*/

class P2
{
	public static void main(String[] args) 
	{
		int n=5;
		for (int i=0; i<n; i++)
		{
			int k=0;
			int m=1;
			for (int j=0; j<n; j++)
			{
				if (i+j<=n-1)
				{
					if (k%2==1)
					{
						System.out.print("*");
					}
					else
					{
						System.out.print(m++);
					}
					k++;
				}
				else
				{
					System.out.print(" ");
				}
			}
			System.out.println();
		}
	}
}
