class A
{
	int i;
	float j;
	char l;
	double m;

	A()
	{
	}

	A(int i, float j, char l, double m)
	{
		this.i=i;
		this.j=j;
		this.l=l;
		this.m=m;
	}

	public String toString()
	{
		return i + "," + j + "," + l + "," + m;
	}
}

class P3
{
	public static void main(String[] args) 
	{
		A ob = new A();
		A ob1 = new A(10, 10.5f, 'a', 25.5);
		System.out.println(ob);
		System.out.println(ob1);
	}
}
